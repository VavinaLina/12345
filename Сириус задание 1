A=int(input("Введите год, в котором произошло первое событие."))
B=int(input("Введите год, в котором произошло второе событие."))
n=B-A
if n>0 and A<0 and B>0:
    print("Второе событие произошло через",n+1,"после",A)
elif n>0 and A>0 and B>0:
    print("Второе обытие произошло через",n,"после",A)
elif n<0 and A>0 and B<0:
    print("Второе событие произошло на",(n-1)*-1,"раньше",A)
elif n<0 and A<0 and B<0:
    print("Второе событие произошло на",(n)*-1,"раньше",A)